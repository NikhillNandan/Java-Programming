import java.util.*;
public class Main {
    public static void main (String[] args){
        Scanner sc = new Scanner(System.in);
        String name = sc.nextLine();
        String rev = "";
        int len = name.length();
        for(int i=len-1;i>=0;i--){
            rev=rev+name.charAt(i);
        }
        System.out.println(rev);
        sc.close();
    }
}
